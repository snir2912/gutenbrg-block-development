{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "blockylicious/curvy",
  "version": "0.1.0",
  "title": "Curvy",
  "category": "widgets",
  "icon": "smiley",
  "description": "Curvy shape deviders.",
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "link": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "style": {
      "type": "object",
      "default": {
        "color": {
          "background": "red"
        },
        "spacing": {
          "padding": {
            "top": "50px",
            "bottom": "50px",
            "left": "50px",
            "right": "50px"
          }
        }
      }
    },
    "enableTopCurve": {
      "type": "boolean",
      "default": true
    },
    "topWidth": {
      "type": "number",
      "default": 100
    },
    "topHeight": {
      "type": "number",
      "default": 100
    },
    "topFlipX": {
      "type": "boolean",
      "default": false
    },
    "topFlipY": {
      "type": "boolean",
      "default": false
    },
    "topColor": {
      "type": "string",
      "default": "#FFFFFF"
    }
  },
  "textdomain": "blockylicious",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}